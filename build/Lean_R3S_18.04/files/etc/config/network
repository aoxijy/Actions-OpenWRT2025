
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'eth1'
	list ports 'dheth0'

config device
	option name 'eth1'
	option macaddr '00:E0:4C:17:29:77'

config interface 'wan'
